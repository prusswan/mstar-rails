= form_for @song_part do |f|
  - if @song_part.errors.any?
    #error_explanation
      %h2= "#{pluralize(@song_part.errors.count, "error")} prohibited this song_part from being saved:"
      %ul
        - @song_part.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :part_id
    = f.number_field :part_id
  .field
    = f.label :song_id
    = f.collection_select :song_id, Song.all, :id, :title
  .actions
    = f.submit 'Save'
