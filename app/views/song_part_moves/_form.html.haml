= form_for @song_part_move do |f|
  - if @song_part_move.errors.any?
    #error_explanation
      %h2= "#{pluralize(@song_part_move.errors.count, "error")} prohibited this song_part_move from being saved:"
      %ul
        - @song_part_move.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :song_part_id
    = f.collection_select :song_part_id, SongPart.all, :id, :description
  .field
    = f.label :order_id
    = f.number_field :order_id    
  .field
    = f.label :move_id
    = f.collection_select :move_id, Move.all, :id, :mstar_id
  .actions
    = f.submit 'Save'
