= form_for @move do |f|
  - if @move.errors.any?
    #error_explanation
      %h2= "#{pluralize(@move.errors.count, "error")} prohibited this move from being saved:"
      %ul
        - @move.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :mstar_id
    = f.text_field :mstar_id
  .field
    = f.label :sequence
    = f.text_field :sequence, class: 'sequence'
  .actions
    = f.submit 'Save'
